<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="mainpane" focusTraversable="true" maxHeight="1080.0" maxWidth="1920.0" minHeight="576.0"
            minWidth="1024.0"
            prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.GameController">
	<Canvas fx:id="frame_buff" focusTraversable="true" onKeyPressed="#press" onKeyReleased="#release"
	        AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
	        AnchorPane.topAnchor="0.0">
		<cursor>
			<Cursor fx:constant="NONE"/>
		</cursor>
	</Canvas>
</AnchorPane>
